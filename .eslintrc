{
  "extends": ["airbnb"],
  "quotes": [2, "single", "avoid-escape"],
  "plugins": [
    "meteor",
    "jsx-a11y"
  ],
  "env": {
    "meteor": true,
    "node": true
  },
  "rules": {
    "react/prefer-es6-class": 0
  },
  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaVersion": 6,
    "ecmaFeatures": {
      "jsx": true,
      "experimentalObjectRestSpread": true
    }
  }
}
